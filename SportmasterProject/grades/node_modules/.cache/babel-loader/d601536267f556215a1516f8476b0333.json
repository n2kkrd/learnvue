 'chart.js';\r\n  \r\n  export default {\r\n    name: 'StudentChart',\r\n    props: {\r\n      studentData: {\r\n        type: Array,\r\n        required: true\r\n      }\r\n    },\r\n    data: () => ({\r\n      chartData: {}\r\n    }),\r\n    mounted() {\r\n      this.createChart();\r\n    },\r\n    methods: {\r\n      createChart() {\r\n        this.chartData = {\r\n          labels: this.studentData.map(student => student.name),\r\n          data: this.studentData.map(student => student.grade)\r\n        };\r\n  \r\n        // Bar chart\r\n        const barChart = new Chart(this.$refs.barChart, {\r\n          type: 'bar',\r\n          data: {\r\n            labels: this.chartData.labels,\r\n            datasets: [{\r\n              label: 'Оценки студентов',\r\n              data: this.chartData.data,\r\n              backgroundColor: '#2196f3'\r\n            }]\r\n          },\r\n          options: {\r\n            responsive: true,\r\n            maintainAspectRatio: false,\r\n            scales: {\r\n              yAxes: [{\r\n                ticks: {\r\n                  beginAtZero: true\r\n                }\r\n              }]\r\n            }\r\n          }\r\n        });\r\n  \r\n        // Pie chart\r\n        const pieChart = new Chart(this.$refs.pieChart, {\r\n          type: 'pie',\r\n          data: {\r\n            labels: this.chartData.labels,\r\n            datasets: [{\r\n              data: this.chartData.data,\r\n              backgroundColor: [\r\n                '#f44336',\r\n                '#9c27b0',\r\n                '#3f51b5',\r\n                '#009688',\r\n                '#ff5722',\r\n                '#795548',\r\n                '#607d8b',\r\n                '#00bcd4',\r\n                '#cddc39',\r\n                '#4caf50',\r\n                '#ffeb3b',\r\n                '#2196f3'\r\n              ]\r\n            }]\r\n          },\r\n          options: {\r\n            responsive: true,\r\n            maintainAspectRatio: fa